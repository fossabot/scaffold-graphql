{
  "name": "koa-template",
  "version": "0.1.0",
  "description": "koa template for graphql api",
  "main": "index.js",
  "dependencies": {
    "apollo-server-koa": "^2.0.0",
    "bcrypt": "^3.0.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.1.1",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-cache-control": "^2.0.0",
    "koa-compose": "^4.1.0",
    "koa-router": "^7.4.0",
    "merge-graphql-schemas": "^1.5.3",
    "pg": "^7.4.3",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-cache-control": "^2.0.1",
    "@types/koa-router": "^7.0.31",
    "@types/node": "^8.0.29",
    "@types/ramda": "^0.25.36",
    "ava": "^0.25.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-register": "^6.26.0",
    "chance": "^1.0.16",
    "supertest": "^3.1.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-microsoft-contrib": "^5.1.0",
    "tsutils": "^3.0.0",
    "typescript": "^2.5.2"
  },
  "scripts": {
    "test": "ava",
    "lint": "tslint src/**/*.ts",
    "start": "ts-node src/index.ts"
  },
  "keywords": [
    "koa",
    "typescript",
    "graphql"
  ],
  "ava": {
    "files": [
      "test/graphql/**/*.test.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": true,
    "tap": false,
    "verbose": true,
    "powerAssert": false,
    "babel": "inherit",
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "babel-register",
      "./test/helpers/globals.js"
    ]
  },
  "author": "",
  "license": "MIT"
}
